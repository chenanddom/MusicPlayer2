<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#aaaaaa"
    tools:context="player.dom.com.musicplayer.MainActivity">
<com.dom.player.view.MSlidingMenu
    android:id="@+id/mSliding"
    android:layout_width="wrap_content"
    android:layout_height="match_parent">
    <include android:id="@+id/menu" layout="@layout/left_menu"/>
    <include android:id="@+id/content" layout="@layout/content"/>
</com.dom.player.view.MSlidingMenu>
</RelativeLayout>
